{
    "installCommand": "npm install",
    "version": 3,
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"src/**/*",
            "use":"@vercel/static-build"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ]
}